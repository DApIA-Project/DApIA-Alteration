{
  "name": "server",
  "version": "0.16",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.test.ts --timeout 10000",
    "test:coverage": "nyc --reporter=html ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "serve": "node build/server.js",
    "start": "ts-node index.ts",
    "bundle-server": "esbuild index.ts --bundle --platform=node --outfile=build/server.js",
    "bundle": "npm run bundle-server",
    "build": "tsc && npm run bundle",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "dev": "nodemon app.ts",
    "copy:fs": "npx shx cp -fr ./node_modules/fs/ ./public/fs",
    "preinstall": "cd ../alteration-scenario && npm i && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^2.0.12",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "react-dom": "^18.2.0",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@dapia-project/data-converter": "^3.3.0",
    "@smartesting/alteration-scenario": "file:../alteration-scenario",
    "@smartesting/shared": "file:../shared",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.15.13",
    "express": "^4.18.2",
    "monaco-editor": "^0.34.1",
    "monaco-languageclient": "^4.0.3",
    "react": "^18.2.0"
  }
}
